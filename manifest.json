{
  "manifest_version": 3,
  "name": "ScreenshotTaker",
  "version": "1.0",

  "action": {
    "default_popup": "index.html",
    "default_icon": "icon1.png"
},
  "permissions": ["tabs", "storage", "unlimitedStorage", "sessions", "webRequest", "webRequestBlocking", "<all_urls>", "downloads"]
}